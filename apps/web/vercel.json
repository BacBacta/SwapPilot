{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && pnpm --filter @swappilot/shared build && pnpm --filter @swappilot/web build",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "@swappilot_api_url"
  }
}
